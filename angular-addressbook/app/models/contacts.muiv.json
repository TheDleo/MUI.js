{
    "mui": { "template": "views/meta/datagrid.html" },


    "model": "model.contacts",

    "columns": [
        {
            "label": "Name",
            "model": "name"
        },
        {
            "label": "Age",
            "model": "age"
        },
        {
            "cell": { "glyphicon": "edit" },
            "action": { "state": "main.acontactHTML", "params": [ "id", "$id" ] }
        },
        {
            "cell": { "glyphicon": "edit" },
            "action": { "state": "main.acontactGen", "params": [ "id", "$id" ] }
        }
    ],

    "options": {
        "enableRowSelection": true,
        "multiSelect": false,
        "enableColumnReordering": true,
        "showColumnMenu": true
    }
}
